{
  "name": "webpack-seed",
  "version": "1.0.0",
  "description": "Frontend engineering solution based on webpack",
  "main": "server/app",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev-pages": "node-dev server/app --dev --pages",
    "dev-comps": "node-dev server/app --dev --comps",
    "start-dev": "node-dev server/app --dev --comps --pages",
    "start-quiet": "node-dev server/app --dev --quiet",
    "build": "gulp --prod --pages",
    "start-release": "node server/app --prod --pages",
    "deploy": "gulp deploy"
  },
  "keywords": [
    "webpack"
  ],
  "author": "weeast",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.5",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.15.0",
    "colors": "^1.1.2",
    "cookie-parser": "^1.4.1",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "express-http-proxy": "^0.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.0",
    "gulp-clean": "^0.3.1",
    "gulp-htmlmin": "^1.1.3",
    "gulp-jshint": "^1.11.2",
    "gulp-replace": "^0.5.3",
    "gulp-sftp": "^0.1.4",
    "gulp-util": "^3.0.6",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "image-webpack-loader": "^1.6.3",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jquery": "1.11.3",
    "jshint-stylish": "^2.0.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "morgan": "^1.7.0",
    "open": "0.0.5",
    "serve-favicon": "^2.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
